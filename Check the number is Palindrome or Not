using System;

public class HelloWorld{
  public static void Main(string[] args){
    //palindrome
    int a = Convert.ToInt32(Console.ReadLine());
    int org = a;
    int rev = 0;
    while(a!=0){
      rev = rev*10+a%10;
      a = a/10;
    }
    if(rev==org){
      Console.WriteLine("Palindrome");
    }
    else{
      Console.WriteLine("Not a Palindrome");
    }
  }
}
