using System;
using System.Text.RegularExpressions;
using System.Collections.Generic;

public class HelloWorld{
  public static void Main(string[] args){
    int num = Convert.ToInt32(Console.ReadLine());
    int sum = 0;
    for(int i=0; i<=num; i++){
      sum = sum+i;
    }
    Console.WriteLine(sum);
  }
}
