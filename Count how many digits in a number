using System;
using System.Text.RegularExpressions;
using System.Collections.Generic;

public class HelloWorld{
  public static void Main(string[] args){
    int num = Convert.ToInt32(Console.ReadLine());
    int count=0;
    while(num!=0){
      num = num/10;
      count++;
      
    }
    Console.WriteLine(count);
  }
}
